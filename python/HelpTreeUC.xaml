<UserControl x:Class="scripthea.python.HelpTreeUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:scripthea.python"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        
        <TreeView Name="HelpTree" TreeViewItem.Selected="TreeViewItem_Selected" SizeChanged="HelpTree_SizeChanged">
        </TreeView>
        <Label FontSize="14" VerticalAlignment="Top" HorizontalAlignment="Right" Content="Help" Margin="0,0,5,0" FontFamily="Segoe UI Semibold" Foreground="#FF016B0A"></Label>
    </Grid>
</UserControl>
