<Window x:Name="controlAPIwin" x:Class="scripthea.engineAPI.ControlAPI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:scripthea.engineAPI"
        mc:Ignorable="d"
        Title="Text-to-image generator:" Height="450" Width="800" WindowStyle="ToolWindow" WindowStartupLocation="CenterScreen" FontSize="12" Closing="controlAPIwin_Closing">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="40"/>
        </Grid.RowDefinitions>
        <TabControl x:Name="tabControl" Background="{x:Null}" ></TabControl>
        <Button x:Name="btnOK" Content="OK" HorizontalAlignment="Right" Margin="0,7.949,17.127,7.152" VerticalAlignment="Center" Width="90" Grid.Row="1" Height="25" FontWeight="Bold" IsDefault="True" Click="btnOK_Click"/>
        <Button x:Name="btnCancel" Content="Cancel" HorizontalAlignment="Right" Margin="0,7.949,127.127,7.152" VerticalAlignment="Center" Width="95" Grid.Row="1" Height="25" IsCancel="True" Click="btnCancel_Click"/>
    </Grid>
</Window>
